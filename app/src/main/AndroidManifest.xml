<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.roshan.design_safeperiods">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="SafePeriods"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.HOME" />
            </intent-filter>
        </activity>
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".FirstView" />
        <activity android:name=".Activity_BlogDashboard_Doctor" />
        <activity android:name=".Activity_BlogDashboard_User" />
        <activity android:name=".ActivityDoctorlogin" />
        <activity android:name=".ActivityUserlogin" />
        <activity android:name=".ActivityUserRegistration" />
        <activity android:name=".ActivityDoctorRegistration"></activity>
    </application>

</manifest>